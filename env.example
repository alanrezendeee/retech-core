# Configuração do Servidor
PORT=8080
ENV=development

# MongoDB
MONGO_URI=mongodb://localhost:27017
MONGO_DB=retech_core

# Redis (opcional)
REDIS_URL=redis://localhost:6379

# JWT
JWT_ACCESS_SECRET=dev-access-secret-change-in-production
JWT_REFRESH_SECRET=dev-refresh-secret-change-in-production
APIKEY_HASH_SECRET=dev-apikey-hash-secret-change-in-production

# CORS
CORS_ENABLE=true

# APIs Externas (deixe vazio para usar defaults)
CEP_PRIMARY_PROVIDER=viacep
CEP_PRIMARY_URL=https://viacep.com.br
CEP_FALLBACK_PROVIDER=brasilapi
CEP_FALLBACK_URL=https://brasilapi.com.br
CEP_TIMEOUT=5s

CNPJ_PRIMARY_PROVIDER=brasilapi
CNPJ_PRIMARY_URL=https://brasilapi.com.br
CNPJ_FALLBACK_PROVIDER=receitaws
CNPJ_FALLBACK_URL=https://www.receitaws.com.br
CNPJ_TIMEOUT=10s

